{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "entry",
        "corejs": {
          "version": 3,
          "proposals": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "vant",
        "libraryDirectory": "es",
        "style": true
      }
    ],
    "@babel/plugin-transform-modules-commonjs",
    "@babel/plugin-transform-runtime",
    "add-module-exports",
    ["@babel/plugin-proposal-optional-chaining"]
  ]
}
